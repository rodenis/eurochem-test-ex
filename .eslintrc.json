{
    "rules": {
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "generator-star-spacing": [2, {"before": false, "after": true}]
    },
    "env": {
        "node": true,
        "browser": true
    },
    "ecmaFeatures": {
        "generators": true
    },
    "extends": "eslint:recommended"
}